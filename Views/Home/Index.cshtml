@{
    ViewData["Title"] = "Home Page";
    Layout = "_AppLayout";
}
<section class="site-hero overlay" style="background-image: url(/images/home_page.jpg)" data-stellar-background-ratio="0.5">
    <div class="container">
        <div class="row site-hero-inner justify-content-center align-items-center">
            <div class="col-md-10 text-center" data-aos="fade-up">
                <span class="custom-caption text-uppercase text-white d-block  mb-3">Welcome To StyleSense</span>
                <h1 class="heading">Where Fashion Meets Your Sense of Style!</h1>
            </div>
        </div>
    </div>

    <a class="mouse smoothscroll" href="#next">
        <div class="mouse-icon">
            <span class="mouse-wheel"></span>
        </div>
    </a>
</section>
<!-- END section -->
<section class="section">
    <div class="container">
        <div class="row justify-content-center text-center mb-5">
            <div class="col-md-7">
                <h2 class="heading" data-aos="fade-up">Virtual styling sessions are now available!</h2>
                <p data-aos="fade-up" data-aos-delay="100">Book virtual styling sessions with our experienced fashion stylists who will guide you in choosing the right outfits, accessories, and beauty looks to suit your style and occasion. Our virtual styling sessions allow you to receive personalized style advice from the comfort of your own home.</p>
                <a asp-controller="Consultation" asp-action="Create" class="btn btn-primary text-white py-2 mr-3">Book Now</a>
            </div>
        </div>
    </div>
</section>

<section class="py-5 bg-light" id="next">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-12 col-lg-7 ml-auto order-lg-2 position-relative mb-5" data-aos="fade-up">
                <figure class="img-absolute">
                    <img src="/images/welcome2.jpg" alt="Free Website Template by Templateux" class="img-fluid">
                </figure>
                <img src="/images/welcome1.jpg" alt="Image" class="img-fluid rounded">
            </div>
            <div class="col-md-12 col-lg-4 order-lg-1" data-aos="fade-up">
                <h2 class="heading">Welcome!</h2>
                <p class="mb-4">Welcome to our online fashion stylist website! We are here to help you elevate your personal style and create fashionable looks that make you feel confident and stylish. Our website offers a range of services and resources to assist you in achieving your fashion goals.</p>
                <p class="mb-4">
                    At our website, you'll find a range of services and resources designed to assist you in achieving your fashion goals. From personalized styling sessions to curated fashion guides, we're here to provide you with the tools you need to enhance your wardrobe and express your unique sense of style.
                </p>
                <p><a asp-controller="StyleSense" asp-action="Services" class="btn btn-primary text-white py-2 mr-3">Our Services</a>
            </div>

        </div>
    </div>
</section>
